{
  "manifest_version": 3,
  "name": "CPH Submit",
  "description": "Codeforces Submit add-on for Competitive Programming Helper.",
  "version": "1.5.0",
  "icons": {
    "48": "icon-48.png"
  },
  "action": {
    "default_icon": "icon-48.png"
  },
  "background": {
    "service_worker": "./dist/backgroundScript.js"
  },
  "permissions": [
    "webNavigation",
    "scripting",
    "tabs",
    "storage",
    "activeTab",
    "cookies"
  ],
  "host_permissions": [
    "*://localhost/*",
    "*://codeforces.com/*"
  ]
}